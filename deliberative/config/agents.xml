<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### DELIBERATIVE AGENTS ### -->
	<agent name="deliberative-naive">
		<set class-path="bin/"/>
		<set class-name="template.DeliberativeTemplate"/>
		<set algorithm="NAIVE"/>
	</agent>
	
	<agent name="deliberative-bfs">
		<set class-path="bin/"/>
		<set class-name="template.DeliberativeTemplate"/>
		<set algorithm="BFS"/>
	</agent>
	
	<agent name="deliberative-astar-optimistic">
		<set class-path="bin/"/>
		<set class-name="template.DeliberativeTemplate"/>
		<set algorithm="ASTAR"/>
		<set heuristic="OPTIMISTIC" />
	</agent>
	
<agent name="deliberative-astar-constant">
		<set class-path="bin/"/>
		<set class-name="template.DeliberativeTemplate"/>
		<set algorithm="ASTAR"/>
  		<set heuristic="CONSTANT" />   
	</agent>
	
	<agent name="deliberative-astar-delivery">
		<set class-path="bin/"/>
		<set class-name="template.DeliberativeTemplate"/>
		<set algorithm="ASTAR"/>
		<set heuristic="DELIVERY" /> 
	</agent>
	
</agents>
