<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### CENTRALIZED AGENTS ### -->
	<agent name="centralized-Naive">
		<set class-path="bin/"/>
		<set class-name="template.CentralizedTemplate"/>
		<set algorithm="NAIVE"/>
	</agent>
	
	<agent name="centralized-SLS">
		<set class-path="bin/"/>
		<set class-name="template.CentralizedTemplate"/>
		<set algorithm="SLS"/>
		<set probability="1"/>
		<set bound="50000"/>
		<set stall="5000"/>
	</agent>		 
	
	<agent name="centralized-SLSrandom">
		<set class-path="bin/"/>
		<set class-name="template.CentralizedTemplate"/>
		<set algorithm="SLS_RANDOM_INITIAL"/>
		<set probability="1"/>
		<set bound="50000"/>
		<set stall="5000"/>
	</agent>
	
</agents>
