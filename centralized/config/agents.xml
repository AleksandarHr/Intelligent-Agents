<?xml version="1.0" encoding="ISO-8859-1"?>

<agents>
	<!-- A list of agents and their behavior classes -->
	
	<!-- #### CENTRALIZED AGENTS ### -->
	<agent name="centralized-Naive">
		<set class-path="bin/"/>
		<set class-name="template.CentralizedTemplate"/>
		<set algorithm="NAIVE"/>
	</agent>
	
	<agent name="centralized-SLS">
		<set class-path="bin/"/>
		<set class-name="template.CentralizedTemplate"/>
		<set algorithm="SLS"/>
		<set probability="0.7"/>
	</agent>
	
	<agent name="centralized-SLSrandom">
		<set class-path="bin/"/>
		<set class-name="template.CentralizedTemplate"/>
		<set algorithm="SLS_RANDOM_INITIAL"/>
		<set probability="0.7"/>
	</agent>
	
	<agent name="centralized-SLSgenetic">
		<set class-path="bin/"/>
		<set class-name="template.CentralizedTemplate"/>
		<set algorithm="SLS_GENETIC"/>
		<set populationSize="100" />
	</agent>
	
</agents>
